<script>
    document.addEventListener('DOMContentLoaded', () => {
        const popup = document.getElementById('popup')
        const form = document.getElementById('form')
        let formOffset = form.offsetTop
        let formHeight = form.offsetHeight

        window.addEventListener('resize', () => {
            formOffset = form.offsetTop
            formHeight = form.offsetHeight
        })

        document.addEventListener('scroll', () => {
            let offset = window.pageYOffset + window.innerHeight
            if(offset > window.innerHeight && offset < formOffset || offset > formOffset + formHeight) {
                popup.classList.add('active')
            } else {
                popup.classList.remove('active')
            }
        })

        makeTimer(600)
    })
</script>
